<div class="headnav">
<span<b><%= link_to "GRADE U", root_path %></b></span>
<span<b><%= link_to "SIGN IN", sign_in_path, class: "pull-right" %></b></span>
</div>
<div class="form-wrap">
   <% if @user.errors.any? %>
      <% @user.errors.full_messages.each do |s| %>
         <div class="alert alert-danger" role="alert"><%= s %></div>
         <% end %>
   <% end %>
   <div class="panel panel-default">
     <div class="panel-heading">Registration</div>
     <div class="formpad">
<%= form_for @user, url: users_path do |f| %>
  <div class="form-group">


    <%= f.label :lastname, "Last Name" %><br/>
    <%= f.text_field :lastname, class: "form-control" %></br>


    <%= f.label :email, "Email" %><br/>
    <%= f.text_field :email, class: "form-control" %></br>


    <%= f.label :password, "Password" %><br/>
    <%= f.text_field :password, class: "form-control" %></br>

    <%= f.submit "Register", class: "btn btn-primary btn-block"%>
  </div>
<% end %>
</div>
</div>
</div>
